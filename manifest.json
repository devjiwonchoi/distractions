{
  "manifest_version": 3,
  "name": "",
  "version": "0.0.0",
  "description": "No shorts, no comments, clean.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "src/index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "src/index.js"
      ]
    }
  ]
}